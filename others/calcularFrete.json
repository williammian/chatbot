{
  "name": "calcularFrete",
  "description": "Calcula o frete de acordo com a quantidade de produtos e a UF do cliente",
  "parameters": {
    "type": "object",
    "properties": {
      "quantidadeProdutos": {
        "type": "number",
        "description": "Quantidade de produtos"
      },
      "uf": {
        "type": "string",
        "description": "UF da cidade do cliente",
        "enum": [
          "AC",
          "AL",
          "AP",
          "AM",
          "BA",
          "CE",
          "DF",
          "ES",
          "GO",
          "MA",
          "MT",
          "MS",
          "MG",
          "PA",
          "PB",
          "PR",
          "PE",
          "PI",
          "RJ",
          "RN",
          "RS",
          "RO",
          "RR",
          "SC",
          "SP",
          "SE",
          "TO"
        ]
      }
    },
    "required": [
      "quantidadeProdutos",
      "uf"
    ]
  }
}